	public Object loggerAOP(ProceedingJoinPoint joinPoint) throws Throwable{
		String methodname = joinPoint.getSignature().toString();
		System.out.println("system start");
		
		long st = System.currentTimeMillis();
		
		
		
		return joinPoint;
		
		
	}
	
}
